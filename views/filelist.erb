<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <link rel="stylesheet" href="/stylesheets/main.css">
    <title>Dynamic Directory Index</title>
  </head>
  
  <body>
    <table id='list'>
      <tr id='title'><td><h3>Here are all available files!</h3></td></tr>
      <% @list.each do |name| %>
        <tr><td><%= name %></td></tr>
      <% end %>
    </table>
  </body>
</html>
